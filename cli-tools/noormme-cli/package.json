{
	"name": "noormme-cli",
	"version": "1.0.0",
	"description": "NOORMME CLI - Seamless Next.js template deployment with automatic setup",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"noormme": "./dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.ts",
		"start": "node dist/index.js",
		"clean": "rm -rf dist",
		"prebuild": "npm run clean",
		"postbuild": "chmod +x dist/index.js",
		"test": "echo \"Tests not implemented yet\"",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix"
	},
	"keywords": [
		"noormme",
		"nextjs",
		"sqlite",
		"cli",
		"template",
		"deployment",
		"automation"
	],
	"author": "NOORMME Team",
	"license": "MIT",
	"dependencies": {
		"chalk": "^5.3.0",
		"commander": "^11.1.0",
		"fs-extra": "^11.2.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@types/fs-extra": "^11.0.4",
		"typescript": "^5.3.0",
		"tsx": "^4.6.0",
		"eslint": "^8.55.0",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/noormme/noormme-cli.git"
	},
	"bugs": {
		"url": "https://github.com/noormme/noormme-cli/issues"
	},
	"homepage": "https://github.com/noormme/noormme-cli#readme"
}
