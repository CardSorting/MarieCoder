{
	"name": "noormme-nextjs-template",
	"version": "1.0.0",
	"description": "A clean Next.js template with NOORMME integration following NORMIE DEV methodology",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"type-check": "tsc --noEmit",
		"setup": "npm run migrate && npm run seed",
		"dev:full": "npm run setup && npm run dev",
		"reset": "rm -f database.sqlite && npm run setup",
		"migrate": "tsx scripts/migrate.ts",
		"seed": "tsx scripts/seed.ts"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.0.0",
		"@radix-ui/react-dropdown-menu": "^2.0.0",
		"@radix-ui/react-select": "^2.0.0",
		"@radix-ui/react-switch": "^1.0.0",
		"@radix-ui/react-tabs": "^1.0.0",
		"@radix-ui/react-toast": "^1.0.0",
		"@radix-ui/react-tooltip": "^1.0.0",
		"autoprefixer": "^10.4.0",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"date-fns": "^2.30.0",
		"kysely": "^0.27.0",
		"lucide-react": "^0.300.0",
		"nanoid": "^5.1.6",
		"next": "^15.0.0",
		"next-auth": "^4.24.0",
		"noormme": "latest",
		"postcss": "^8.4.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"recharts": "^2.8.0",
		"sqlite3": "^5.1.0",
		"tailwindcss": "^3.4.0",
		"tailwindcss-animate": "^1.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"eslint": "^8.0.0",
		"eslint-config-next": "^15.0.0",
		"tsx": "^4.0.0",
		"typescript": "^5.0.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
