{
	"name": "marie-coder",
	"displayName": "MarieCoder",
	"description": "AI coding agent for the rest of us. Built on Cline, Create files, run commands, browse the web - all with your permission every step of the way.",
	"version": "3.32.6",
	"icon": "assets/icons/Marie-icon.png",
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "MarieCoder Team"
	},
	"license": "Apache-2.0",
	"publisher": "cardsorting",
	"repository": {
		"type": "git",
		"url": "https://github.com/CardSorting/MarieCoder"
	},
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"Marie",
		"Marie Coder",
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"beginner-friendly",
		"easy-to-use"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished",
		"workspaceContains:evals.env"
	],
	"main": "./dist/extension.js",
	"bin": {
		"mariecoder": "./dist-cli/mariecoder.js"
	},
	"contributes": {
		"walkthroughs": [
			{
				"id": "marieWalkthrough",
				"title": "Meet Marie Coder, your AI coding partner",
				"description": "Marie Coder makes coding accessible to everyone. Here are 5 ways to get started:",
				"steps": [
					{
						"id": "welcome",
						"title": "Start with a Goal, Not Just a Prompt",
						"description": "Tell Marie Coder what you want to achieve. It plans, asks, and then codes, like a true partner.",
						"media": {
							"markdown": "walkthrough/step1.md"
						}
					},
					{
						"id": "learn",
						"title": "Let Marie Coder Learn Your Codebase",
						"description": "Point Marie Coder to your project. It builds understanding to make smart, context-aware changes.",
						"media": {
							"markdown": "walkthrough/step2.md"
						}
					},
					{
						"id": "advanced-features",
						"title": "Always Use the Best AI Models",
						"description": "Marie Coder empowers you with State-of-the-Art AI, connecting to top models (Anthropic, Gemini, OpenAI & more).",
						"media": {
							"markdown": "walkthrough/step3.md"
						}
					},
					{
						"id": "mcp",
						"title": "Extend with Powerful Tools (MCP)",
						"description": "Connect to databases, APIs, or discover new capabilities in the MCP Marketplace.",
						"media": {
							"markdown": "walkthrough/step4.md"
						}
					},
					{
						"id": "getting-started",
						"title": "You're Always in Control",
						"description": "Review Marie Coder's plans and diffs. Approve changes before they happen. No surprises.",
						"media": {
							"markdown": "walkthrough/step5.md"
						},
						"content": {
							"path": "walkthrough/step5.md"
						}
					}
				]
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "marie-coder-ActivityBar",
					"title": "Marie Coder",
					"icon": "assets/icons/Marie-dev-icon.svg"
				}
			]
		},
		"views": {
			"marie-coder-ActivityBar": [
				{
					"type": "webview",
					"id": "marie-coder.SidebarProvider",
					"name": "",
					"icon": "assets/icons/Marie-dev-icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "marie.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "marie.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(server)"
			},
			{
				"command": "marie.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "marie.accountButtonClicked",
				"title": "Account",
				"icon": "$(account)"
			},
			{
				"command": "marie.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "marie.dev.createTestTasks",
				"title": "Create Test Tasks",
				"category": "Marie Coder",
				"when": "marie.isDevMode"
			},
			{
				"command": "marie.addToChat",
				"title": "Add to Marie Coder",
				"category": "Marie Coder"
			},
			{
				"command": "marie.addTerminalOutputToChat",
				"title": "Add to Marie Coder",
				"category": "Marie Coder"
			},
			{
				"command": "marie.focusChatInput",
				"title": "Jump to Chat Input",
				"category": "Marie Coder"
			},
			{
				"command": "marie.generateGitCommitMessage",
				"title": "Generate Commit Message with Marie Coder",
				"category": "Marie Coder",
				"icon": {
					"light": "assets/icons/Marie-dev-icon-light.svg",
					"dark": "assets/icons/Marie-dev-icon-dark.svg"
				}
			},
			{
				"command": "marie.abortGitCommitMessage",
				"title": "Generate Commit Message with Marie Coder - Stop",
				"category": "Marie Coder",
				"icon": "$(debug-stop)"
			},
			{
				"command": "marie.explainCode",
				"title": "Explain with Marie Coder",
				"category": "Marie Coder"
			},
			{
				"command": "marie.improveCode",
				"title": "Improve with Marie Coder",
				"category": "Marie Coder"
			},
			{
				"command": "marie.openWalkthrough",
				"title": "Open Walkthrough",
				"category": "Marie Coder"
			},
			{
				"command": "marie.reconstructTaskHistory",
				"title": "Reconstruct Task History",
				"category": "Marie Coder"
			}
		],
		"keybindings": [
			{
				"command": "marie.addToChat",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "editorHasSelection"
			},
			{
				"command": "marie.generateGitCommitMessage",
				"when": "config.git.enabled && scmProvider == git"
			},
			{
				"command": "marie.focusChatInput",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "!editorHasSelection"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "marie.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == marie-coder.SidebarProvider"
				},
				{
					"command": "marie.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == marie-coder.SidebarProvider"
				},
				{
					"command": "marie.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == marie-coder.SidebarProvider"
				},
				{
					"command": "marie.accountButtonClicked",
					"group": "navigation@5",
					"when": "view == marie-coder.SidebarProvider"
				},
				{
					"command": "marie.settingsButtonClicked",
					"group": "navigation@6",
					"when": "view == marie-coder.SidebarProvider"
				}
			],
			"editor/context": [
				{
					"command": "marie.addToChat",
					"group": "navigation",
					"when": "editorHasSelection"
				}
			],
			"terminal/context": [
				{
					"command": "marie.addTerminalOutputToChat",
					"group": "navigation"
				}
			],
			"scm/title": [
				{
					"command": "marie.generateGitCommitMessage",
					"group": "navigation",
					"when": "config.git.enabled && scmProvider == git && !marie.isGeneratingCommit"
				},
				{
					"command": "marie.abortGitCommitMessage",
					"group": "navigation",
					"when": "config.git.enabled && scmProvider == git && marie.isGeneratingCommit"
				}
			],
			"commandPalette": [
				{
					"command": "marie.generateGitCommitMessage",
					"when": "config.git.enabled && scmProvider == git && !marie.isGeneratingCommit"
				},
				{
					"command": "marie.abortGitCommitMessage",
					"when": "config.git.enabled && scmProvider == git && marie.isGeneratingCommit"
				}
			]
		},
		"configuration": {
			"title": "Marie Coder",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.mjs",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.mjs --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run build:webview && npm run lint && node esbuild.mjs --production",
		"cli:build": "node esbuild.cli.mjs --production",
		"cli:dev": "node esbuild.cli.mjs",
		"cli:watch": "node esbuild.cli.mjs --watch",
		"cli": "node dist-cli/mariecoder.js",
		"clean:build": "rimraf dist dist-cli dist-standalone webview-ui/build src/generated out/",
		"clean:deps": "rimraf node_modules webview-ui/node_modules",
		"clean:all": "npm run clean:build && npm run clean:deps",
		"compile-tests": "node ./scripts/build-tests.js",
		"watch-tests": "tsc -p . -w --outDir out",
		"check-types": "npx tsc --noEmit && cd webview-ui && npx tsc -b --noEmit",
		"lint": "biome lint --no-errors-on-unmatched --files-ignore-unknown=true --diagnostic-level=error",
		"format": "biome format --changed --no-errors-on-unmatched --files-ignore-unknown=true --diagnostic-level=error",
		"format:fix": "biome check --changed --no-errors-on-unmatched --files-ignore-unknown=true --write",
		"fix:all": "biome check --no-errors-on-unmatched --files-ignore-unknown=true --write --diagnostic-level=error --unsafe",
		"ci:check-all": "npm-run-all -p check-types lint format",
		"ci:build": "npm run build:webview && node esbuild.mjs && npm run compile-tests",
		"pretest": "npm run compile && npm run compile-tests && npm run lint",
		"test": "npm-run-all test:unit test:integration",
		"test:integration": "vscode-test",
		"test:unit": "cross-env TS_NODE_PROJECT=./tsconfig.unit-test.json mocha",
		"test:coverage": "vscode-test --coverage",
		"e2e": "playwright test -c playwright.config.ts",
		"test:e2e:build": "vsce package --allow-package-secrets sendgrid --out dist/e2e.vsix",
		"test:e2e": "playwright install && npm run test:e2e:build && node src/test/e2e/utils/build.mjs && playwright test",
		"test:e2e:optimal": "npm run test:e2e:build && node src/test/e2e/utils/build.mjs && playwright test",
		"test:e2e:ui": "npx tsx scripts/interactive-playwright.ts",
		"install:all": "npm install && cd webview-ui && npm install",
		"dev:webview": "cd webview-ui && npm run dev",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish --allow-package-secrets sendgrid && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --allow-package-secrets sendgrid --pre-release && ovsx publish --pre-release",
		"publish:marketplace:nightly": "node ./scripts/publish-nightly.mjs",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"docs": "cd docs && npm run dev",
		"docs:check-links": "cd docs && npm run check",
		"docs:rename-file": "cd docs && npm run rename",
		"report-issue": "node scripts/report-issue.js"
	},
	"lint-staged": {
		"*": [
			"biome check --write --staged --no-errors-on-unmatched --files-ignore-unknown=true"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@bufbuild/buf": "^1.54.0",
		"@changesets/cli": "^2.27.12",
		"@playwright/test": "^1.53.2",
		"@types/better-sqlite3": "^7.6.13",
		"@types/chai": "^5.0.1",
		"@types/diff": "^5.2.1",
		"@types/get-folder-size": "^3.0.4",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/pdf-parse": "^1.1.4",
		"@types/proxyquire": "^1.3.31",
		"@types/should": "^11.2.0",
		"@types/sinon": "^17.0.4",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.84.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.5.2",
		"@vscode/vsce": "^3.6.0",
		"c8": "^10.1.3",
		"chai": "^4.3.10",
		"chalk": "5.6.2",
		"cross-env": "^10.1.0",
		"esbuild": "^0.25.0",
		"grpc-tools": "^1.13.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.0",
		"minimatch": "^3.0.3",
		"npm-run-all": "^4.1.5",
		"nyc": "^17.1.0",
		"prebuild-install": "^7.1.3",
		"protoc-gen-ts": "^0.8.7",
		"proxyquire": "^2.1.3",
		"rimraf": "^6.0.1",
		"should": "^13.2.3",
		"sinon": "^19.0.2",
		"tree-kill": "^1.2.2",
		"ts-node": "^10.9.2",
		"ts-proto": "^2.6.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.37.0",
		"@grpc/grpc-js": "^1.14.0",
		"@modelcontextprotocol/sdk": "^1.11.1",
		"@streamparser/json": "^0.0.22",
		"@vscode/codicons": "^0.0.36",
		"better-sqlite3": "^12.4.1",
		"chokidar": "^4.0.1",
		"chrome-launcher": "^1.1.2",
		"default-shell": "^2.2.0",
		"diff": "^5.2.0",
		"exceljs": "^4.4.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"https-proxy-agent": "^7.0.6",
		"iconv-lite": "^0.6.3",
		"ignore": "^7.0.3",
		"image-size": "^2.0.2",
		"isbinaryfile": "^5.0.2",
		"jschardet": "^3.1.4",
		"linkedom": "^0.18.12",
		"mammoth": "^1.8.0",
		"nice-grpc": "^2.1.13",
		"nice-grpc-common": "^2.0.2",
		"open": "^10.1.2",
		"openai": "^4.83.0",
		"os-name": "^6.0.0",
		"p-timeout": "^6.1.4",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"reconnecting-eventsource": "^1.6.4",
		"simple-git": "^3.27.0",
		"strip-ansi": "^7.1.2",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"ulid": "^2.4.0",
		"vscode-uri": "^3.1.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.24.2"
	},
	"c8": {
		"reporter": [
			"lcov",
			"html"
		],
		"exclude": [
			"**/testing-platform/**",
			"**/webview-ui/**",
			"**/.vscode-test/**",
			"**/node_modules/**",
			"node_modules",
			"**/dist-standalone/src/**",
			"**/dist-standalone/vsce-extension/https:/**",
			"**/dist-standalone/vsce-extension/**",
			"**/dist-standalone/https:/**",
			"**/dist-standalone/LIB/src/**",
			"**/dist-standalone/pdfjs-dist/**",
			"**/*.d.ts",
			"**/*.{test,spec}.{js,jsx,ts,tsx,mjs,cjs}",
			"**/__tests__/**",
			"**/test/**",
			"**/tests/**",
			"**/.nyc_output/**",
			"**/tests-results/**",
			"src/test/**",
			"**/src/xml/**",
			"**/standalone/**",
			"**/src/generated/**",
			"**/evals/cli/dist/**",
			"**/evals/cli/src/**",
			"dist"
		],
		"all": true,
		"exclude-after-remap": true
	}
}
