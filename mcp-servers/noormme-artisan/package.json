{
	"name": "noormme-artisan",
	"version": "1.0.0",
	"description": "NOORMME Artisan MCP Server - Laravel-style commands for Next.js development",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"clean": "rm -rf dist",
		"dev": "tsc --watch",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write src/",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"mcp",
		"noormme",
		"artisan",
		"nextjs",
		"development"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.11.1",
		"fs-extra": "^11.2.0",
		"chalk": "^4.1.2",
		"commander": "^11.1.0",
		"inquirer": "^8.2.6",
		"ora": "^5.4.1"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^8.2.10",
		"@types/node": "^20.11.19",
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.57.0",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"ts-jest": "^29.1.2",
		"typescript": "^5.3.3"
	}
}
